{
  "name": "@weavlet/agi-achieved",
  "version": "0.0.1",
  "description": "Check if AGI has been achieved. Spoiler: not yet.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "agi",
    "artificial-general-intelligence",
    "skynet",
    "placeholder",
    "soon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/weavlet/weavlet"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}